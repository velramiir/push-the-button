    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^23.0.1",
    "naive-ui": "^2.40.4",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.49",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.25"
